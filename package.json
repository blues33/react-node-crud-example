  },
  "scripts": {
    "build-css": "node-sass-chokidar --importer=./node_modules/node-sass-tilde-importer --include-path ./node_modules ./src/global/scss -o ./src/global/scss",
    "watch-css": "npm run build-css && node-sass-chokidar --importer=./node_modules/node-sass-tilde-importer --include-path ./node_modules ./src/global/scss -o ./src/global/scss --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "server": "nodemon ./server/server.js --exec babel-node",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
